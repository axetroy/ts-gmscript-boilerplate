{
  "name": "ts-gmscript-boilerplate",
  "version": "0.1.0",
  "description": "gm script boilerplate build with typescript and webpack",
  "scripts": {
    "test": "echo 'Test success!'",
    "build": "webpack --display-error-details --progress --colors --mode production",
    "start": "webpack --display-error-details --progress --colors --watch --mode development",
    "contributor": "./node_modules/.bin/contributors-stat",
    "generate": "all-contributors generate",
    "deploy": "gh-pages -d ./dist"
  },
  "keywords": [
    "typescript",
    "script"
  ],
  "author": "Axetroy",
  "license": "ISC",
  "devDependencies": {
    "all-contributors-cli": "4.0.1",
    "contributors-stat": "1.1.1",
    "gh-pages": "2.2.0",
    "moment": "2.24.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {}
}
